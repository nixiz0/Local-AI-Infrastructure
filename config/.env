# === Host ports ===
QDRANT_HTTP_PORT=5333
QDRANT_GRPC_PORT=5334
OLLAMA_PORT=10434
OPENWEBUI_PORT=3000

# === Models to be pre-pulled on the Ollama side ===
OLLAMA_MODELS=deepseek-r1:8b, nomic-embed-text:latest, huihui_ai/deepseek-r1-abliterated:8b

# === Embeddings & STT (Open WebUI) ===
RAG_EMBEDDING_ENGINE=ollama
RAG_EMBEDDING_MODEL=nomic-embed-text
WHISPER_MODEL=large-v3
WHISPER_VAD_FILTER=true
WHISPER_LANGUAGE=fr